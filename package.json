		"@typescript-eslint/eslint-plugin": "^5.17.0",
		"date-fns": "^3.6.0",
		"eslint": "^8",
		"eslint-config-next": "14.2.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-cypress": "^3.3.0",
		"eslint-plugin-jest-dom": "^5.4.0",
		"eslint-plugin-testing-library": "^6.2.2",
